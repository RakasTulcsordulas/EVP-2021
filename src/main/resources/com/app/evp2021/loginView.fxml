<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.app.evp2021.controllers.LoginController">
    <Label fx:id="welcomeText" text="Welcome"/>
    <TextArea fx:id="username" maxWidth="300" maxHeight="0" promptText="Username"/>
    <PasswordField fx:id="password" maxWidth="300" maxHeight="0" promptText="Password"/>
    <Button fx:id="logIn" text="Log in" onAction="#onLogInClick"/>
    <Label fx:id="missing" text="Minden mezőt ki kell tölteni!" visible="false"/>
</VBox>
